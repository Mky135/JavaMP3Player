<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.mp3PlayerController">
   <children>
      <Label fx:id="songName" alignment="CENTER" layoutX="100.0" layoutY="45.0" prefWidth="200.0" text="Song name" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS" wrapText="true">
         <font>
            <Font name="Avenir Heavy Oblique" size="21.0" />
         </font></Label>
      <ImageView fx:id="image" fitHeight="314.0" fitWidth="343.0" layoutX="29.0" layoutY="143.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="playButton" layoutX="171.0" layoutY="457.0" mnemonicParsing="false" text="Stopped" onAction="#play"/>
      <Button layoutX="271.0" layoutY="457.0" mnemonicParsing="false" text="Skip" onAction="#skip"/>
   </children>
</AnchorPane>
